<!-- DataTables Example -->
<div class="card mb-3" id="registered">
  <div class="card-header" id="admin-table-head">
    <i class="fas fa-table"></i>
    Registered Students Records</div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>S/N</th>
            <th>Name</th>
            <th>Student ID</th>
            <th>Phone Numbers</th>
            <th>class</th>
            <th>gender</th>
            <th>view</th>
            <th>edit</th>
          </tr>
        </thead>
        <tfoot>
          <tr>
            <th>S/N</th>
            <th>Name</th>
            <th>Student ID</th>
            <th>Phone Numbers</th>
            <th>class</th>
            <th>gender</th>
            <th>view</th>
            <th>edit</th>
          </tr>
        </tfoot>
        <tbody>
           <% if(result.length > 0){ %> 
            <% result.forEach((details,index)=>{ %>
          <tr>
            <td><%= index %> </td>
            <td><%= details.surname%> <%= details.childsname%></td>
            <td><%= details.studentID%></td>
            <td><%= details.phonenumber%></td>
            <td><%= details.childclass%></td>
            <td><%= details.gender%></td>
            <td><a href="/studentID/<%= details._id %> "> view</a></td>
            <td> <a href="/studentID/<%= details.id %>/edit ">edit</a></td>
            
          </tr>
         
           <% }) %>
            <% }else{ %>  
                <p> no record found </p>
                <% } %> 
        </tbody>
      </table>
    </div>
  </div>
  <!-- <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div> -->
</div>



